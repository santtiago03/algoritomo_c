#include  <stdio.h>
#include <stdlib.h>

int main () {
	
	float fahr; 

	printf ("Informe  a temperatura em fahrenheit. :");
	scanf ("%f", &fahr);
	
	float celsius = (fahr - 32) / 1.8; 

	printf("\n a temperatura em fahrenheit e : %f", fahr);
   
    printf("\n A temperatura em celsius e : %f", celsius);

return 0;
