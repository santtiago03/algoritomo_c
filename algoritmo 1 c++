#include  <stdio.h>
#include <stdlib.h>

int main () {
	float nota1;
	float nota2;
	float nota3;
	float nota4; 
	
	printf ("Informe a primeira nota :");
	scanf ("%f", &nota1);
	printf ("Informe a segunda nota :");
	scanf ("%f", &nota2 );
	printf ("Informe a terceira nota :");
	scanf ("%f", &nota3);
	printf ("Informe a quarta nota :");
	scanf ("%f", &nota4);
	
	float total = nota1+nota2+nota3+nota4;
	printf("A nota total e :%f",total);
	
return 0;
} 
